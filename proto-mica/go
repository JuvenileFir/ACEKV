echo "本命令将执行server实验程序,不写入log"
if [ $# == 1 ]
then
    if [ $1 == "-p" ]
    then
        echo 将打印过程信息
        sudo ./main -f 2 -l 35
    elif [ $1 == "-h" ]
    then
        echo "当且仅当参数个数为1且为 -p 时打印过程信息,除 -h/p 外其余情况均正常执行"
    else
        echo 仅执行,不打印过程信息
        sudo ./main
    fi
else
    echo 仅执行,不打印过程信息
    sudo ./main
fi